{
  "manifest_version": 3,
  "name": "Intents New Tab",
  "version": "2.1.0",
  "description": "Replace your new tab page with Intents - a minimal, distraction-free search experience",
  "permissions": [],
  "host_permissions": [
    "https://api.duckduckgo.com/*",
    "https://duckduckgo.com/*",
    "https://en.wikipedia.org/*",
    "https://export.arxiv.org/*",
    "https://api.stackexchange.com/*",
    "https://api.github.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}